\mode<presentation>

\newif\ifbeamer@titlehead
\beamer@titleheadtrue
\newif\ifbeamer@frametitlebg
\beamer@frametitlebgtrue
\newif\ifbeamer@addurl
\beamer@addurlfalse

\DeclareOptionBeamer{compress}{\beamer@compresstrue}
\DeclareOptionBeamer{notitlehead}{\beamer@titleheadfalse}
\DeclareOptionBeamer{noframetitlebg}{\beamer@frametitlebgfalse}
\DeclareOptionBeamer{addurl}{\beamer@addurltrue}
\ProcessOptionsBeamer

\useoutertheme{fawkesminimal}
\useinnertheme{fawkesminimal}
\usecolortheme{fawkes}

%\setbeamertemplate{footline}[infolines theme]
\setbeamerfont{block title}{size={}}
\setbeamertemplate{items}[square]
\setbeamertemplate{sections/subsections in toc}[square]
 
\mode
<all>
